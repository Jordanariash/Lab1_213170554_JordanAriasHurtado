#lang racket

;pendiente, corregir type station, solo recibe 'r en vez de r

;Dominio: id (int) X name (String)  X type (station-type) X stop-time (positive integer)
;Recorrido: station

(define valid '(r m c t))

(define (station-type type)
  (member type valid)
  )

(define (station id name type stop-time)
  (if (and (integer? id)
           (string? name)
           (station-type type)
           (integer? stop-time) (>= stop-time 0)
           )
      (list id name type stop-time)
      null)
  )